#include<iostream>
using namespace std;

main()
{
	// calculation of grades
	int marks;
	cout<<"enter your marks:"<<"\n";
	cin>>marks;
	if(marks>=90)
	{
		cout<<"you got A grade"<<"\n";
	}
	else if(marks>=80)
	{
		cout<<"you got B grade"<<"\n";
	}
	else if(marks>=70)
	{
		cout<<"you got B grade"<<"\n";
	}
	else if(marks>=60)
	{
		cout<<"you got C grade"<<"\n";
	}
	else if(marks>=50)
	{
		cout<<"you got D grade"<<"\n";
	}
	else
   {
		cout<<"you got F grade"<<"\n";
	}
	return 0;
}
